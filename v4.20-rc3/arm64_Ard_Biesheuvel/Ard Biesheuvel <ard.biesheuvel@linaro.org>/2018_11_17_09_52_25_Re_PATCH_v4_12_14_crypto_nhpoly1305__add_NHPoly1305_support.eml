> +               u32 m3 =3D get_unaligned_le32(message + 12);
> +
> +               sums[0] +=3D (u64)(u32)(m0 + key[ 0]) * (u32)(m2 + key[ 2=
]);
> +               sums[1] +=3D (u64)(u32)(m0 + key[ 4]) * (u32)(m2 + key[ 6=
]);
> +               sums[2] +=3D (u64)(u32)(m0 + key[ 8]) * (u32)(m2 + key[10=
]);
> +               sums[3] +=3D (u64)(u32)(m0 + key[12]) * (u32)(m2 + key[14=
]);
> +               sums[0] +=3D (u64)(u32)(m1 + key[ 1]) * (u32)(m3 + key[ 3=
]);
> +               sums[1] +=3D (u64)(u32)(m1 + key[ 5]) * (u32)(m3 + key[ 7=
]);
> +               sums[2] +=3D (u64)(u32)(m1 + key[ 9]) * (u32)(m3 + key[11=
]);
> +               sums[3] +=3D (u64)(u32)(m1 + key[13]) * (u32)(m3 + key[15=
]);
> +               key +=3D NH_MESSAGE_UNIT / sizeof(key[0]);
> +               message +=3D NH_MESSAGE_UNIT;
> +               message_len -=3D NH_MESSAGE_UNIT;
> +       }
