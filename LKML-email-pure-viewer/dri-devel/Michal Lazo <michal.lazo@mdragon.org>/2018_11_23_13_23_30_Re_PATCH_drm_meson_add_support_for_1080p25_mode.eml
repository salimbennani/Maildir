> >>>       { 5, &meson_hdmi_encp_mode_1080i60 },
> >>>       { 20, &meson_hdmi_encp_mode_1080i50 },
> >>>       { 32, &meson_hdmi_encp_mode_1080p24 },
> >>> +     { 33, &meson_hdmi_encp_mode_1080p50 },
